<!-- Price Forecasting Tables -->
<div class="container">
    <div class="row">
        <div class="col-md-9">
            <div class="panel panel-primary">
                <div class="panel-heading ">
                    <h5 style="font-weight: bold;">Price Forecasting one-day ahead</h5>
                </div>
                <div class="panel-body">
                   

                    <table style="width:100%" class="table table-striped " id="customers" >
                      <thead>
                        <tr>
                          <th style = "background-color:rgb(31, 156, 88);" >Time</th>
                          <% var l = day.length   %> 
                          <% for(var i = 0; i< l/2;i++) { %>            
                            <th scope="col"><%= day[i] %>:00</th>        
                          <%  } %>
                      </tr>
                      </thead>
                      <tbody>
                     <tr scope="row">  
                     <th style = "background-color:rgb(31, 156, 88);">Price</th>
                     <% for(var i = 0; i< l/2;i++) { %>            
                            <td scope="col"><%= prices[i] %></td>        
                          <%  } %>
                      </tr>
                      </tbody>
                
                </table>
                
                 <table style="width:100%" class="table table-striped " id="customers" >
                      <thead>
                        <tr>
                          <th style = "background-color:rgb(31, 156, 88);">Time</th>
                          <% for(var i = 0; i< l/2;i++) { %>            
                            <th scope="col"><%= day[i + l/2 ] %>:00</th>        
                          <%  } %>
                      </tr>
                      </thead>
                      <tbody>
                     <tr scope="row">  
                     <th style = "background-color:rgb(31, 156, 88);">Price</th>
                     <% for(var i = 0; i< l/2;i++) { %>            
                            <td scope="col"><%= prices[i+l/2] %></td>        
                          <%  } %>
                      </tr>
                      </tbody>
                
                </table>



                </div>
            </div>
        </div>


<!-- Load Management  Section-->

        <div class="col-md-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h5 style="font-weight: bold;">Load Management</h5>
                </div>
                <div class="box">
                    <%  newListItems.forEach(function(item){ %>                      
                        <form action="/delete" method="post">
                        <div class="item">    
                          <input type="checkbox" name ="checkflag" onChange="this.form.submit()">                                               
                          <input type="hidden" name="checkbox" value="<%=item.name%>" >
                          <p ><%=item.name%></p>
                          
                        </div>
                       
                      </form>        

                    <%  }) %>
                  
                  
                        <form class="item" action="/" method="post">
                          <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                          <button type="submit" name="list" >+</button>
                        </form>
                    </div>




                </div>
            </div>
        </div>
